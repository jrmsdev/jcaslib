SCRIPTSD = ../scripts

.PHONY: all
all: build


.PHONY: build
build:
	@$(SCRIPTSD)/mk-fordirs.sh build


.PHONY: clean
clean:
	@$(SCRIPTSD)/mk-fordirs.sh clean


.PHONY: distclean
distclean:
	@$(SCRIPTSD)/mk-fordirs.sh distclean


.PHONY: depend
depend:
	@$(SCRIPTSD)/mk-fordirs.sh depend


.PHONY: clean-depend
clean-depend:
	@$(SCRIPTSD)/mk-fordirs.sh clean-depend
